# These are the default error messages for NASPID

E_MOUNT=(2 "Failure")

E_ROOT=(3 "Must be run as root")

E_ARGS=(4 "Improper usage. [ $0 {save} <Source> ]")

E_CLEAN=(5 "Cleanup of un-used source files failed")

E_CONFIG=(6 "Missing or unset configuration")

E_FSTAB=(7 "Something went wrong writing fstab for")

E_DEP=(8 "You have unmet dependancies")

E_USAGE=(9 "USAGE: [ $0 {stop|start|restart|reload}]")
