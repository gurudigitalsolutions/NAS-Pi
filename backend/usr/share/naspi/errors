# These are the default error messages for NASPID

# Userid error
E_ROOT=(3 "$PROG must be run as root")
# Usage error
E_USAGE=(4 "Usage: $0 {fstab|status} <Source>")
# Missing source files
E_SOURCE=(5 "Missing source files")
# Missing config files
E_CONFIG=(6 "Missing or unset configuration")
# Empty data error
E_DATA=(7 "Empty data found")
# Disconnected device error
E_DEVICE=(8 "Device not connected")
# Empty control file error
E_CONTROL=(9 "Unsupported control file format")
# Mounting error
E_MOUNT=(10 "Failed to mount")
